<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?android:attr/selectableItemBackground"
    android:padding="@dimen/dp_2"
    >


    <com.github.johnkil.print.PrintView
        android:id="@+id/arrow_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignWithParentIfMissing="true"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/node_selector"
        app:iconColor="#12558C"
        app:iconFont="fonts/material-icon-font.ttf"
        app:iconSize="20dp"
        app:iconText="@string/ic_keyboard_arrow_right" />

    <TextView
        android:layout_marginStart="@dimen/dp_30"
        android:id="@+id/node_selector"
        android:layout_width="@dimen/dp_24"
        android:layout_height="@dimen/dp_24"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:textColor="#00000000"
        android:layout_centerVertical="true"
        android:text="1"
        android:button="@drawable/ic_check_box_yes1"
        android:visibility="gone" />

    <!--<com.github.johnkil.print.PrintView-->
        <!--android:id="@+id/icon"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_alignWithParentIfMissing="true"-->
        <!--android:layout_centerHorizontal="true"-->
        <!--android:layout_centerVertical="true"-->
        <!--android:layout_marginLeft="5dp"-->
        <!--android:layout_toRightOf="@id/arrow_icon"-->
        <!--app:iconColor="#12558C"-->
        <!--app:iconFont="fonts/material-icon-font.ttf"-->
        <!--app:iconSize="24dp"-->
        <!--app:iconText="@string/ic_email" />-->

    <ImageView
        android:id="@+id/img_file"
        android:layout_alignWithParentIfMissing="true"
        android:layout_centerHorizontal="true"
        android:layout_width="@dimen/dp_22"
        android:layout_height="@dimen/dp_22"
        android:layout_centerVertical="true"
        android:layout_marginStart="@dimen/dp_2"
        android:src="@drawable/ic_insert_drive_01"
        android:layout_toEndOf="@+id/node_selector"
        />


    <TextView
        android:id="@+id/node_value"
        android:layout_alignWithParentIfMissing="true"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="@dimen/dp_2"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_14"
        android:layout_toEndOf="@+id/img_file"
       />
</RelativeLayout>
